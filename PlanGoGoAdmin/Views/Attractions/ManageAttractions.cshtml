@{
    Layout = "~/Views/shared/_cityLayout.cshtml";
}
@Scripts.Render("~/controllerAttractions")
<div ng-controller="controllerAttractions">

    <table cellpadding="0" cellspacing="0" border="0" class="gridTable">
        <tr>
            <td colspan="2">
                <table class="gridTable" cellpadding="5px">
                    <tbody class="gridHeader">
                        <tr>
                            <th>

                            </th>
                            <th>
                                Attraction Name
                            </th>
                            <th>
                                Address One
                            </th>
                            <th>
                                Address Two
                            </th>
                            <th>
                                Longitude
                            </th>
                            <th>
                                Latitude
                            </th>
                            <th>
                                PlaceId
                            </th>
                            <th>
                                RankId
                            </th>
                            <th>
                                Google Search Text
                            </th>
                            <th>
                                Google Web Site
                            </th>
                            <th>
                                Google ICon
                            </th>
                            <th>
                                Google International phone number
                            </th>
                            <th>
                                Googleadr address
                            </th>
                            <th>
                                Google Name
                            </th>
                            <th>
                                Google Rating
                            </th>
                            <th>
                                Google User Ratings total
                            </th>
                        </tr>
                    </tbody>
                    <tr ng-repeat="_attractions in Attractions">
                        <td>
                            <a href="/AttractionTravelTimeDistance/ManageAttractionTravelTimeDistance?AttractionsId={{_attractions.AttractionsId}}" title="Update Attractions">
                                Nearest Attractions
                            </a>
                            <a href="/OpenTime/ManageOpenTime?AttractionsId={{_attractions.AttractionsId}}" title="Open Time">
                                Open Time
                            </a>                            
                            <a href="/Attractions/UpdateAttractions?CityId={{_attractions.AttractionsId}}" title="Update Attractions">
                                Edit
                            </a>
                        </td>                        
                        <td title="'Attraction Name'">
                            {{_attractions.AttractionName}}
                        </td>
                        <td title="'Address One'">
                            {{_attractions.AddressOne}}
                        </td>
                        <td title="'Address Two'">
                            {{_attractions.AddressTwo}}
                        </td>
                        <td title="'Longitude'">
                            {{_attractions.Longitude}}
                        </td>
                        <td title="'Latitude'">
                            {{_attractions.Latitude}}
                        </td>
                        <td title="'Place Id'">
                            {{_attractions.PlaceId}}
                        </td>
                        <td title="'Google Search Text'">
                            {{_attractions.GoogleSearchText}}
                        </td>
                        <td title="'Google Web Site'">
                            {{_attractions.GoogleWebSite}}
                        </td>
                        <td title="'Google ICon'">
                            {{_attractions.GoogleICon}}
                        </td>
                        <td title="'Google International phone number'">
                            {{_attractions.GoogleInternational_phone_number}}
                        </td>
                        <td title="'Googleadr_address'">
                            {{_attractions.Googleadr_address}}
                        </td>
                        <td title="'Google Name'">
                            {{_attractions.GoogleName}}
                        </td>
                        <td title="'Google Rating'">
                            {{_attractions.GoogleRating}}
                        </td>
                        <td title="'GoogleUser ratings total'">
                            {{_attractions.GoogleUser_ratings_total}}
                        </td>                        
                    </tr>
                </table>
            </td>
        </tr>
    </table>


</div>

