<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/bundles/jquery", "~/bundles/jqueryui", "~/bundles/jqueryval")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/angularSupport")
    @Styles.Render("~/Content/themes/base/css")

    @Scripts.Render("~/customscript")
    
    

    <script>
  $( function() {
    $( "#menu" ).menu();
  } );
    </script>
</head>
<body ng-app="AppPlanGoGo">
    <div class="adminMainHeader">
    <div id="MainDev" class="mainMenu">
        <!--Menu List-->
        <div style="float: left">
            <ul id="menu" class="bodyContent">
                <li>
                    <div>Manage Users</div>
                    <ul>
                        <li>
                            <a href="~/User/ManageUser" title="Manage User">
                                View Users
                            </a>
                        </li>
                        <li>
                            <a href="~/User/AddUpdateUser?adminUserId=0" title="Update User">
                                Add User
                            </a>
                        </li>
                    </ul>
                </li>
                <li>
                    <div>Break Information</div>
                    <ul>
                        <li>
                            <a href="~/BreakInformation/ManageBreakInformation" title="Manage Break Info">
                                View Break Information
                            </a>
                        </li>
                        <li>
                            <a href="~/BreakInformation/UpdateBreakInformation?breakInformationId=0" title="Update Break Info">
                                Update Break Information
                            </a>
                        </li>
                    </ul>
                </li>
                <li>
                    <div>Master Travel Types</div>
                    <ul>
                        <li>
                            <a href="~/MasterTravelMode/ManageTravelMode" title="Master Travel Types">
                                View Travel Types
                            </a>
                        </li>
                        <li>
                            <a href="~/MasterTravelMode/UpdateMasterTravelMode?travelModeId=0" title="Update Travel Types">
                                Update Travel Types
                            </a>
                        </li>
                    </ul>
                </li>
                <li>
                    <div>Manage Category</div>
                    <ul>
                        <li>
                            <a href="~/MasterCategory/ManageMasterCategory" title="Manage Category">
                                View Category
                            </a>
                        </li>
                        <li>
                            <a href="~/MasterCategory/UpdateMasterCategory?Category=0" title="Update Category">
                                Update Category
                            </a>
                        </li>
                    </ul>
                </li>
                <li>
                    <div>Manage Google Category</div>
                    <ul>
                        <li>
                            <a href="~/MasterGoogleType/ManageMasterGoogleType" title="Manage Google Category">
                                View Google Category
                            </a>
                        </li>
                        <li>
                            <a href="~/MasterGoogleType/UpdateMasterGoogleType?googleTypeID=0" title="Update Google Category">
                                Update Google Category
                            </a>
                        </li>
                    </ul>
                </li>
                <li>
                    <div>Manage Country</div>
                    <ul>
                        <li>
                            <a href="~/Country/ManageCountry" title="Manage Country">
                                View Country
                            </a>
                        </li>
                        <li>
                            <a href="~/Country/AddCountry?CountryId=0" title="Update Country">
                                Add Country
                            </a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
        <div style="float: left;" class="bodyContent">
            @RenderBody()
        </div>
    </div>
    </div>
</body>
</html>
