            <endpoint address="http://localhost/PlanGoGoService/Admin/AttractionTravelTimeDistance.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAttractionTravelTimeDistance"
                contract="AdminAttractionTravelTime.IAttractionTravelTimeDistance"
                name="BasicHttpBinding_IAttractionTravelTimeDistance" />
