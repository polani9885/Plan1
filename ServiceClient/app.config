            <endpoint address="http://localhost/PlanGoGoService/Admin/MissingDistanceAttractionsRecords.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMissingDistanceAttractionsRecords"
                contract="AdminMissingDistanceAttractionsRecords.IMissingDistanceAttractionsRecords"
                name="BasicHttpBinding_IMissingDistanceAttractionsRecords" />
            <endpoint address="http://localhost/PlanGoGoService/Schedulers/UsersService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserServices"
                contract="UsersService.IUserServices" name="BasicHttpBinding_IUserServices" />
