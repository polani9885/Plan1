                <binding name="BasicHttpBinding_IGetListValues" />
                <binding name="BasicHttpBinding_IAttractions" />
                <binding name="BasicHttpBinding_IAttractionsActiveStatus" />
                <binding name="BasicHttpBinding_IAttractionTravelTimeDistance" />
                <binding name="BasicHttpBinding_ICountry" />
                <binding name="BasicHttpBinding_IMasterCategory" />
                <binding name="BasicHttpBinding_IMasterCity" />
                <binding name="BasicHttpBinding_IMasterState" />
                <binding name="BasicHttpBinding_IMasterWeek" />
                <binding name="BasicHttpBinding_IMissingDistanceAttractionsRecords" />
