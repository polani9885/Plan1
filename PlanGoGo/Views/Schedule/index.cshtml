<div id="tab_Filter">
    <table border="0">
        <tr>
            <td>
                <div class="ui-widget" style="float: left; padding: 5px">
                    <input id="autoCityName" placeholder="Enter City Name"> <input type="button" value="Add More Cites" ng-click="addCitites()" />
                    <br />
                    <div id="travelBasic" style="display: none">
                        <input id="autoStartLocation" placeholder="Start Location">
                        <br />
                        <input id="autoDestinationLocation" placeholder="End Location">
                        <br />
                        <input id="calStartDateAndTime" placeholder="Start Date And Time" ng-model="StartDate">
                        <br />
                        <input id="calDropDateAndTime" placeholder="End Date And Time">
                    </div>
                </div>
            </td>
            <td ng-show="VisitCityList.length>0">
                <table>
                    <tr>
                        <td>
                            City Name
                        </td>
                        <td>
                            Order or Visit
                        </td>
                    </tr>
                    <tr ng-repeat="cities in  VisitCityList | orderBy:'recordIndex'">
                        <td>
                            <table>
                                <tr>
                                    <td>
                                        {{cities.cityName}}
                                    </td>
                                    <td>
                                        <img src="/Images/Custome/red_x.png" alt="Not Intereseted" width="30px" height="30px" ng-click="CityNotIntereseted(cities.cityId)" />
                                    </td>
                                    <td>
                                        <img ng-show="!$first" src="/Images/Custome/up.jpg" alt="Up" width="30px" height="30px" ng-click="IsCityOrderUpdated(cities.cityId, cities.recordIndex, true)" />
                                        <img ng-show="!$last" src="/Images/Custome/down.jpg" alt="Down" width="30px" height="30px" ng-click="IsCityOrderUpdated(cities.cityId, cities.recordIndex, false)" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
</div>
<div id="tab_Categories">
    <table>
        <tr>
            <td style="width:300px" valign="top">
                <table>
                    <tr>
                        <td valign="top">
                            <ul class="Catnav" ng-repeat="mainCategoryList in MasterCategoryList">
                                <li ng-class="{'CatNavSelected':CssMainCategory(mainCategoryList.CategoryId)}">
                                    <a href="JavaScript:Void(0)" ng-click="ClickMainCategorySelected(mainCategoryList.CategoryId)">
                                        {{mainCategoryList.CategoryName}}
                                    </a>
                                </li>
                            </ul>
                        </td>
                    </tr>
                </table>
            </td>
            <td valign="top">
                <table ng-show="filterCategoryList.length">
                    <tr>
                        <td valign="top">
                            <table>
                                <tr ng-repeat="categoryList in filterCategoryList" ng-switch on="$index % 5">
                                    <td ng-switch-when="0" valign="top">
                                        <div class="cat_DisplayNewLine" ng-show="filterCategoryList[$index]">
                                            <img src="/Images/User/Category/{{filterCategoryList[$index].TypeName}}.png" alt="{{filterCategoryList[$index].TypeName}}" width="50px" height="50px" />
                                            <input type="checkbox" checked="checked" ng-click="CategorySelected(filterCategoryList[$index])" />
                                            {{filterCategoryList[$index].TypeName}}
                                        </div>
                                    </td>
                                    <td ng-switch-when="0">
                                        <div class="cat_DisplayNewLine" ng-show="filterCategoryList[$index+1]">
                                            <img src="/Images/User/Category/{{filterCategoryList[$index+1].TypeName}}.png" alt="{{filterCategoryList[$index+1].TypeName}}" width="50px" height="50px" />
                                            <input type="checkbox" checked="checked" ng-click="CategorySelected(filterCategoryList[$index+1])" />
                                            {{filterCategoryList[$index+1].TypeName}}
                                        </div>
                                    </td>
                                    <td ng-switch-when="0">
                                        <div class="cat_DisplayNewLine" ng-show="filterCategoryList[$index+2]">
                                            <img src="/Images/User/Category/{{filterCategoryList[$index+2].TypeName}}.png" alt="{{filterCategoryList[$index+2].TypeName}}" width="50px" height="50px" />
                                            <input type="checkbox" checked="checked" ng-click="CategorySelected(filterCategoryList[$index+2])" />
                                            {{filterCategoryList[$index+2].TypeName}}
                                        </div>
                                    </td>
                                    <td ng-switch-when="0">
                                        <div class="cat_DisplayNewLine" ng-show="filterCategoryList[$index+3]">
                                            <img src="/Images/User/Category/{{filterCategoryList[$index+3].TypeName}}.png" alt="{{filterCategoryList[$index+3].TypeName}}" width="50px" height="50px" />
                                            <input type="checkbox" checked="checked" ng-click="CategorySelected(filterCategoryList[$index+3])" />
                                            {{filterCategoryList[$index+3].TypeName}}
                                        </div>
                                    </td>
                                    <td ng-switch-when="0">
                                        <div class="cat_DisplayNewLine" ng-show="filterCategoryList[$index+4]">
                                            <img src="/Images/User/Category/{{filterCategoryList[$index+4].TypeName}}.png" alt="{{filterCategoryList[$index+4].TypeName}}" width="50px" height="50px" />
                                            <input type="checkbox" checked="checked" ng-click="CategorySelected(filterCategoryList[$index+4])" />
                                            {{filterCategoryList[$index+4].TypeName}}
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <input type="text" placeholder="Search Interested Place" ng-keyup="categoryFilter()" ng-model="CategoryPlaceSearch"  /> 
                            <input type="button" ng-click="CategorySelectedAttractionFilter()" value="Show All"/>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <table>
                                <tr ng-repeat="attraction in filterAttractionList" ng-switch on="$index % 2">
                                    <td ng-switch-when="0" valign="top">
                                        <table ng-show="filterAttractionList[$index]" class="attractionInfo">
                                            <tr>
                                                <td rowspan="6">
                                                    <a herf="javascript:void(0)" style="cursor:pointer">
                                                        <img src="~/Images/Custome/intersted.png" alt="Interested" width="100px" height="100px"
