@{
    ViewBag.Title = "index";
    Layout = "~/Views/Shared/_LayoutBeachResort.cshtml";
}
@Scripts.Render("~/index")
<link href="~/Content/AttractionsCategories.css" rel="stylesheet" />
<div id="main" ng-controller="controlerIndex">
<table cellpadding="0" cellspacing="0" border="0">
<tr>
<td>
<div class="divIndex" id="controlerIndex">
<div id="tabs" style="width:1200px;" class="avoidBorder">
<ul>
    <li><a href="#tab_Filter">Filter</a></li>
    <li><a href="#tab_Categories">Categories</a></li>
    <li><a href="#tab_TourPlan" ng-click="TourTabClick()">Tour Plan</a></li>
    <li><a href="#tab_TourMap">Tour Map</a></li>
    <li><a href="#tab_SuggestedAttractions">Suggested Attractions</a></li>
    <li><a href="#tab_BuildStatus">Tour Build Status</a></li>
