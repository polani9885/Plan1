                                                          }>11:00</option>
                                    <option value="11:30" @{
                                                              if (@_attraction.TimeRequiredToView == "11:30")
                                                              {
                                                                  <text>selected</text>
                                                              }
                                                          }>11:30</option>
                                    <option value="12:00" @{
                                                              if (@_attraction.TimeRequiredToView == "12:00")
                                                              {
                                                                  <text>selected</text>
                                                              }
                                                          }>12:00</option>
                                    <option value="13:00" @{
                                                              if (@_attraction.TimeRequiredToView == "13:00")
                                                              {
                                                                  <text>selected</text>
                                                              }
                                                          }>13:00</option>
                                    <option value="14:00" @{
                                                              if (@_attraction.TimeRequiredToView == "14:00")
